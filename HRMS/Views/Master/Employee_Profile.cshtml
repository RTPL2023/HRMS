        function btnDocumentUpload(id) {

            var fup = document.getElementById(id);
            if (fup.value == "") {

                return alert("Choose Pdf File!!");
            }
            else {
                var fileName = fup.value;
                var ext = fileName.substring(fileName.lastIndexOf('.') + 1);
                if (ext == "pdf") {
                    if (fup.files.length != 0) {
                        var fileUpload = $("#"+id).get(0);
                        var files = fileUpload.files;
                        var fileData = new FormData();
                        fileData.append(id, files[0]);
                        fileData.append("employee_id", $("#employee_id").val());

                        $.ajax({
                            url: '/Master/EmployeeDocumentUoload',
                            type: 'post',
                            datatype: 'json',
                            contentType: false,
                            processData: false,
                            async: false,
                            data: fileData,
                            success: function (data) {
                                alert(data);


                                $("#" + id).val("");
                                getemployeeDocument();
                            },
                            error: function (err) {
                                alert("Error Occured");
                            }

                        });
                    }

                }
                else {

                    return false;
                }
            }
        }
